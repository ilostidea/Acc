<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="question" match-catalog="acc"></table-filter>
	<table-filter match-name="answer" match-catalog="acc"></table-filter>
	<table-filter match-name="pump" match-catalog="acc"></table-filter>
	<table-filter match-name="questioncollected" match-catalog="acc"></table-filter>
	<table-filter match-name="answercollected" match-catalog="acc"></table-filter>
	<table-filter match-name="usersetting" match-catalog="acc"></table-filter>
	<table catalog="acc" name="accelement" class="AccElement">
		<column name="ID"></column>
		<column name="AccountingStandardID"></column>
		<column name="CreateTime"></column>
		<column name="ModifyTime"></column>
		<column name="Name"></column>
		<column name="Status"></column>
		<column name="Creator"></column>
		<column name="Modifier"></column>
	</table>
</hibernate-reverse-engineering>